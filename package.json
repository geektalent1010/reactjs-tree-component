{
  "name": "tree-component",
  "version": "1.0.0",
  "description": "A reactjs, angular2 and vuejs tree component.",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p src && tsc -p demo && webpack && npm run rev",
    "lint": "tslint \"src/*.ts\"",
    "rev": "npm run clean-rev && rev-static",
    "clean-rev": "rm -f demo/react.bundle-*.js demo/vue.bundle-*.js demo/angular.bundle-*.js",
    "static": "mkdir ./demo/css && cp -r ./node_modules/jstree/dist/themes/default/ ./demo/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plantain-00/tree-component.git"
  },
  "author": "York Yao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/plantain-00/tree-component/issues"
  },
  "homepage": "https://github.com/plantain-00/tree-component#readme",
  "devDependencies": {
    "@types/react": "15.0.6",
    "@types/react-dom": "0.14.22",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "rev-static": "1.3.1",
    "tslint": "4.4.2",
    "typescript": "2.1.5",
    "vue": "2.1.10",
    "vue-class-component": "4.4.0",
    "webpack": "2.2.1"
  },
  "dependencies": {
    "jstree": "3",
    "tslib": "1"
  }
}
